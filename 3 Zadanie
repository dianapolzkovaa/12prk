using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace _3
{
    internal class ObjectCounter
    {
        static void Main(string[] args)
        {
            ObjectCounter objct = new ObjectCounter();
            ObjectCounter objct1 = new ObjectCounter();
            ObjectCounter objct2 = new ObjectCounter();
            ObjectCounter objct3 = new ObjectCounter();
            ObjectCounter objct4 = new ObjectCounter();
            ObjectCounter objct5 = new ObjectCounter();

            Console.WriteLine($"Количество созданных экземпляров: {num()}");
        }
        public static int count = 0;
        public ObjectCounter()
        {
            count++;
        }

        public static int num()
        {
            return count;
        }
    }
}
